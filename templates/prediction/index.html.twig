{% extends 'base.html.twig' %}

{% block title %}Prediction index{% endblock %}

{% block body %}
    <h1>Prediction index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>HostTeamScores</th>
                <th>GuestTeamScores</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for prediction in predictions %}
            <tr>
                <td>{{ prediction.id }}</td>
                <td>{{ prediction.hostTeamScores }}</td>
                <td>{{ prediction.guestTeamScores }}</td>
                <td>
                    <a href="{{ path('app_prediction_show', {'id': prediction.id}) }}">show</a>
                    <a href="{{ path('app_prediction_edit', {'id': prediction.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_prediction_new') }}">Create new</a>
    <br />
    <a href="{{ path('app_prediction_new_many') }}">Create many</a>
{% endblock %}
